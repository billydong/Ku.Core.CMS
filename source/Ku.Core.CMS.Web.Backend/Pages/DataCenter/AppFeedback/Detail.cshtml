@{
    Layout = "_WindowFormLayout";
}
<div class="layui-layout layui-layout-window">
    @using (Html.BeginForm<DetailModel>(""))
    {
    <div class="layui-body">
        @Html.InputFor(x => x.Dto.Id)
        @Html.ShowFor(x => x.Dto.ProviderName)
        @Html.ShowFor(x => x.Dto.Content)
        @Html.ShowFor(x => x.Dto.CreateTime, x => x.Dto.ResolveTime)
        <hr class="layui-bg-green">
        @Html.InputFor(x => x.Dto.Resolved)
        @Html.InputFor(x => x.Dto.AdminRemark)
    </div>
    <div class="layui-footer">
        @Html.ActionsForSubmitAndClose()
    </div>
    }
</div>
