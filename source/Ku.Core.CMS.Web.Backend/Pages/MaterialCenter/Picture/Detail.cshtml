@{
    Layout = "_WindowFormLayout";
}
<div class="layui-layout layui-layout-window">
    @using (Html.BeginForm<DetailModel>(""))
    {
    <div class="layui-body">
        @Html.ShowFor(x => x.Dto.Title)
        @Html.ShowFor(x => x.Dto.FileName)
        @Html.ShowFor(x => x.Dto.FilePath)
        @Html.ShowFor(x => x.Dto.FileType, x => x.Dto.FileSize)
        @Html.ShowFor(x => x.Dto.Md5Code, x => x.Dto.IsCompressed)
        @Html.ShowFor(x => x.Dto.CreateTime)
    </div>
    <div class="layui-footer">
        @Html.ActionsForClose()
    </div>
    }
</div>
@section Scripts
{
    <script>
        layui.use(['form'], function () {
            var form = layui.form;
        });
    </script>
}
